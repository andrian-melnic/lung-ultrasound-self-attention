{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "resnet18",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "--model", "resnet18",
        "--working_dir_path", ".",
        "--dataset_h5_path", "data/test_dataset_clinic_eval.h5",
        "--hospitaldict_path", "data/test_clinic_eval_hospitaldict.pkl",
        "--rseed", "21",
        "--batch_size", "64",
        "--lr","0.0001",
        "--optimizer", "sgd",
        "--max_epochs", "10",
        "--num_workers", "8",
        "--accumulate_grad_batches", "8",
        "--accelerator", "mps",
        "--precision", "16-mixed"
      ]
    },
    {
      "name": "google_vit",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "--model", "google_vit",
        "--working_dir_path", ".",
        "--dataset_h5_path", "data/test_dataset_clinic_eval.h5",
        "--hospitaldict_path", "data/test_clinic_eval_hospitaldict.pkl",
        "--rseed", "21",
        "--batch_size", "32",
        "--lr","0.0001",
        "--optimizer", "adam",
        "--max_epochs", "10",
        "--num_workers", "8",
        "--accumulate_grad_batches", "8",
        "--accelerator", "mps",
        "--precision", "16-mixed"
